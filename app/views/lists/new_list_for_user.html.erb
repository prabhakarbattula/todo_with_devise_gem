<h1>New list</h1>

<%= form_for @list do |list| %>

   Name: <%= list.text_field :name %> <br>
    Remarks: <%= list.text_area :remarks %> <br>
   <%= list.hidden_field :complete %> <br>
    <%= list.hidden_field :user_id %> <br>

    <%= list.submit "Create List" %>

<% end %>

<%= link_to 'Back', lists_path %>
